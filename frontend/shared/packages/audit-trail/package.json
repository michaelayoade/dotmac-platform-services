{
  "name": "@dotmac/audit-trail",
  "version": "1.0.0",
  "description": "Audit trail and compliance monitoring for DotMac Framework",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@dotmac/http-client": "workspace:*",
    "@dotmac/analytics": "workspace:*",
    "@dotmac/data-tables": "workspace:*",
    "@tanstack/react-query": "^5.28.4",
    "date-fns": "^2.30.0",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "typescript": "^5.9.2",
    "tsup": "^8.3.5",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/jest-dom": "^6.6.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "keywords": [
    "audit",
    "compliance",
    "security",
    "logging",
    "monitoring",
    "dotmac"
